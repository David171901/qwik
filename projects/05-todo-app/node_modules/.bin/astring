#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/david/practice/qwik/qwik-packages/projects/05-todo-app/node_modules/astring/bin/node_modules:/home/david/practice/qwik/qwik-packages/projects/05-todo-app/node_modules/astring/node_modules:/home/david/practice/qwik/qwik-packages/projects/05-todo-app/node_modules:/home/david/practice/qwik/qwik-packages/projects/node_modules:/home/david/practice/qwik/qwik-packages/node_modules:/home/david/practice/qwik/node_modules:/home/david/practice/node_modules:/home/david/node_modules:/home/node_modules:/node_modules:/home/david/practice/qwik/qwik-packages/projects/05-todo-app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/david/practice/qwik/qwik-packages/projects/05-todo-app/node_modules/astring/bin/node_modules:/home/david/practice/qwik/qwik-packages/projects/05-todo-app/node_modules/astring/node_modules:/home/david/practice/qwik/qwik-packages/projects/05-todo-app/node_modules:/home/david/practice/qwik/qwik-packages/projects/node_modules:/home/david/practice/qwik/qwik-packages/node_modules:/home/david/practice/qwik/node_modules:/home/david/practice/node_modules:/home/david/node_modules:/home/node_modules:/node_modules:/home/david/practice/qwik/qwik-packages/projects/05-todo-app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../astring/bin/astring" "$@"
else
  exec node  "$basedir/../astring/bin/astring" "$@"
fi
